        <Button layoutX="100" layoutY="380" minHeight="30" minWidth="200" text="write xml" onAction="#ActionWriteXml" fx:id="write_xml" />
        <Button layoutX="100" layoutY="440" minHeight="30" minWidth="200" text="view tracks" onAction="#ActionWievTracks" fx:id="view_tracs" />
       
       <Label layoutX="330" layoutY="500" minHeight="16" minWidth="69" fx:id="label" />
