    <children>
        <Button layoutX="100" layoutY="80" minHeight="30" minWidth="200"  text="read txt" onAction="#ActionReadTxt" fx:id="read_txt" />
        <Button layoutX="100" layoutY="140" minHeight="30" minWidth="200" text="read xml" onAction="#ActionReadXml" fx:id="read_xml" />
